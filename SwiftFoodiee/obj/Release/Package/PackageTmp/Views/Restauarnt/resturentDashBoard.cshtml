
@{
    ViewBag.Title = "resturentDashBoard";
}
<div class="box">
    <div class="box-header">
        <h3 class="box-title">Dash Board</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <table id="example1" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Customer Name</th>
                    <th>Order Type</th>
                    <th>Location</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Trident</td>
                    <td>InternetExplorer 4.0</td>
                    <td>Win 95+</td>
                    <td> 4</td>
                    <td><button type="button" class="btn btn-success">Accept</button> <button type="button" class="btn btn-warning">Cancel</button></td>

                </tr>
            </tbody>
        </table>
    </div>
    <!-- /.box-body -->
</div>
<script src="~/Scripts/JQuery/JQuery3.3.1/jquery-3.3.1.js"></script>
<script src="~/Scripts/JQuery/JQuery3.3.1/jquery-3.3.1.min.js"></script>
<script>
    $(function () {
        $('#example1').DataTable()
        $('#example2').DataTable({
            'paging': true,
            'lengthChange': false,
            'searching': false,
            'ordering': true,
            'info': true,
            'autoWidth': false
        })
    })
</script>
